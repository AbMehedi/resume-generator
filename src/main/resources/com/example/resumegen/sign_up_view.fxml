<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.resumegen.SignUpController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="213.0" />
      <Text layoutX="299.0" layoutY="88.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User Name" />
      <Text layoutX="300.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email" />
      <Text layoutX="300.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
      <Text layoutX="300.0" layoutY="345.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Already have an account? " />
      <TextField fx:id="Username" layoutX="403.0" layoutY="71.0" promptText="Username" />
      <TextField fx:id="Email" layoutX="402.0" layoutY="112.0" promptText="example@domain.com" />
      <Button fx:id="SignUp" layoutX="448.0" layoutY="238.0" mnemonicParsing="false" onAction="#handleSignUp" text="Sign Up" />
      <Button fx:id="Login" layoutX="459.0" layoutY="328.0" mnemonicParsing="false" onAction="#backToLoginPage" text="Log in" />
      <Text layoutX="299.0" layoutY="217.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Confirm Password" />
      <Label fx:id="label" layoutX="395.0" layoutY="285.0" text="*Please fill all the required fields" textFill="RED" />
      <PasswordField fx:id="Password" layoutX="405.0" layoutY="152.0" promptText="Password" />
      <PasswordField fx:id="ConfirmPassword" layoutX="405.0" layoutY="200.0" promptText="Confirm Password" />
   </children>
</AnchorPane>
