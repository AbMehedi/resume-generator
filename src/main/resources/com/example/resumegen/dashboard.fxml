      <Text layoutX="129.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Build resume with us within a minute">
         <font>
            <Font size="21.0" />
         </font>
      </Text>
      <Button fx:id="createResumeButton" layoutX="274.0" layoutY="188.0" mnemonicParsing="false" onAction="#createResume" text="Create your Resume" />
